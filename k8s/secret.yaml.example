# Example secret file - DO NOT commit actual secrets to git
# Create actual secret with: kubectl create secret generic whatsapp-crm-secrets --from-literal=...

apiVersion: v1
kind: Secret
metadata:
  name: whatsapp-crm-secrets
type: Opaque
stringData:
  supabase-url: "https://your-project.supabase.co"
  supabase-anon-key: "your-anon-key-here"
  supabase-service-key: "your-service-role-key-here"
  jwt-secret: "your-jwt-secret-here"
  nextauth-secret: "your-nextauth-secret-here"
